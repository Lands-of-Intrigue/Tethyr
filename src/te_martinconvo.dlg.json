        "Text": {
          "type": "cexolocstring",
          "value": {
            "0": "Welcome to the Temple of Kelemvor. I am Sir Markar Masterash, a knight of Tethyr and Doomguide, which means I am a priest of the Lord of the Dead. What brings you to us today?"
          }
        }
      },
      {
        "__struct_id": 1,
        "ActionParams": {
          "type": "list",
          "value": []
        },
        "Animation": {
          "type": "dword",
          "value": 0
